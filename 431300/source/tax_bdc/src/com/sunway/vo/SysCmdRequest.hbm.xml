<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sunway.vo.SysCmdRequest" table="sys_cmd_request">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="cmd_code" type="java.lang.String">
            <column name="CMD_CODE" />
        </property>
        <property name="create_time" type="java.util.Date">
            <column name="CREATE_TIME" />
        </property>
        <property name="resolve_state" type="java.lang.Integer">
            <column name="RESOLVE_STATE" />
        </property>
        <property name="resolve_time" type="java.util.Date">
            <column name="RESOLVE_TIME" />
        </property>
        <property name="send_resolve_time" type="java.util.Date">
            <column name="SEND_RESOLVE_TIME" />
        </property>
        <property name="data_id" type="java.lang.String">
            <column name="DATA_ID" />
        </property>
        <property name="fail_msg" type="java.lang.String">
            <column name="FAIL_MSG" />
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="TYPE" />
        </property>        
    </class>
</hibernate-mapping>
